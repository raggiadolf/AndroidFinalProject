<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycompany.dotsproject" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MovesGameActivity"
            android:label="@string/title_activity_moves_game" >
        </activity>
        <activity
            android:name=".HighScoreActivity"
            android:label="@string/title_activity_high_score" >
        </activity>
        <activity
            android:name=".PowerUpsActivity"
            android:label="@string/title_activity_power_ups" >
        </activity>
    </application>

</manifest>
